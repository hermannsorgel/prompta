{
  "name": "prompta",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "dev": "tauri dev",
    "build": "tauri build",
    "//build:debug": "this can be done easily from the CLI, so this is more for documentation",
    "build:debug": "tauri build --debug",
    "ui:dev": "vite dev",
    "ui:build": "cross-env NODE_ENV=production vite build",
    "ui:build-static": "cross-env NODE_ENV=production SVELTE_ADAPTER=static vite build",
    "dev:server": "pnpm tsx --tsconfig ./tsconfig.server.json --watch ./sync-server/server.ts",
    "prod:server": "tsc -p ./tsconfig.server.json; NODE_ENV=production node ./dist-server/server.js",
    "build:server": "tsc -p ./tsconfig.server.json",
    "release": "direnv reload && yarn version && ./scripts/release.sh",
    "gh:publish": "./scripts/publish.sh",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.1",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/adapter-static": "~2.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@tailwindcss/typography": "0.5.9",
    "@tauri-apps/cli": "1.5.8",
    "@types/better-sqlite3": "7.6.8",
    "@types/marked": "^4.0.0",
    "@types/prismjs": "1.26.0",
    "autoprefixer": "~10.4.7",
    "cross-env": "7.0.3",
    "pino-pretty": "10.2.3",
    "postcss": "~8.4.14",
    "postcss-load-config": "~4.0.1",
    "posthog-js": "1.56.0",
    "svelte": "^3.59.1",
    "svelte-check": "~3.4.6",
    "svelte-preprocess": "~4.10.7",
    "tailwindcss": "^3.1.5",
    "tslib": "~2.4.1",
    "tsx": "4.6.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.5",
    "vitest": "~0.31.0"
  },
  "type": "module",
  "dependencies": {
    "@fastify/cors": "8.4.2",
    "@microsoft/fetch-event-source": "2.0.1",
    "@tauri-apps/api": "1.5.2",
    "@vkontakte/vk-qr": "2.0.13",
    "@vlcn.io/crsqlite": "0.15.1",
    "@vlcn.io/crsqlite-wasm": "0.16.0-next.2",
    "@vlcn.io/rx-tbl": "0.15.0-next.0",
    "@vlcn.io/ws-common": "0.1.2",
    "@vlcn.io/xplat-api": "0.14.1",
    "better-sqlite3": "9.2.2",
    "bits-ui": "0.11.8",
    "classnames": "2.3.2",
    "clsx": "2.0.0",
    "cmdk-sv": "0.0.12",
    "fastify": "4.24.3",
    "lucide-svelte": "0.298.0",
    "marked": "^4.0.0",
    "nanoid": "4.0.2",
    "openai": "^4.0.1",
    "prism-svelte": "0.5.0",
    "prismjs": "1.29.0",
    "svelte-markdown": "0.2.3",
    "tailwind-merge": "2.1.0",
    "tailwind-variants": "0.1.19",
    "uuid": "9.0.0"
  }
}
